<table>
  <thead>
  <th></th>
  <th>Name</th>
  <th>Price</th>
  <th>Quantity</th>
  </thead>
  <tbody>
<% @products.each do | product | %>
    <tr><%= image_tag product.image.thumb %></tr>
    <tr><%=	 product.name %></tr>
    <tr>$<%= product.price %></tr>
    <tr><%= product.instock %></tr>
<% end %>
  </tbody>
</table>
